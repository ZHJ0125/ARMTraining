none                      /proc         proc    defaults         0 0
none                      /dev/pts      devpts  mode=0622        0 0
tmpfs                     /dev/shm      tmpfs   defaults         0 0
/dev/mtdblock3            /root         yaffs   defaults         1 1
none                      /sys          sysfs   defaults         0 0
